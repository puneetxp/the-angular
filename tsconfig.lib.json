{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022", "DOM"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": false,
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "inlineSourceMap": true,
    "types": ["google.accounts"],
    "outDir": "../dist/out-tsc/the-angular"
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts",
    "lib/module/angularx-social-login-master/**"
  ],
  "include": [
    "public-api.ts",
    "lib/**/*.ts"
  ],
  "angularCompilerOptions": {
    "compilationMode": "partial"
  }
}
